<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<AnchorPane fx:id="anchorPaneIdM" stylesheets="@css/menuStyle.css" prefHeight="800.0" prefWidth="1400.0"
            xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.sae.sae_juba_antoine_said.Controlleur.ControlleurMenu">
    <children>
        <ImageView fitWidth="${anchorPaneIdM.prefWidth}" fitHeight="${anchorPaneIdM.prefHeight}">
            <image>
                <Image url="@images/imageMenu.png"/>
            </image>
        </ImageView>
        <Button fx:id="lancerButton" layoutX="450.0" layoutY="630.0" onAction="#lancerButtonAction" prefHeight="70.0"
                prefWidth="140.0" text="Lancer"/>
        <Button fx:id="quitterButton" layoutX="750.0" layoutY="630.0" onAction="#quitterButtonAction" prefHeight="70.0"
                prefWidth="140.0" text="Quitter"/>
    </children>
</AnchorPane>