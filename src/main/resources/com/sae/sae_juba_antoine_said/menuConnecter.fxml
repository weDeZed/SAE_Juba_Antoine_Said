    <children>
        <ImageView fitHeight="${anchorPaneIdM.prefHeight}" fitWidth="${anchorPaneIdM.prefWidth}">
            <image>
                <Image url="@images/imageMenu.png" />
            </image>
        </ImageView>
        <Button fx:id="lancerButton" layoutX="450.0" layoutY="630.0" onAction="#lancerButtonAction" prefHeight="70.0" prefWidth="140.0" text="Jouer" />
        <Button fx:id="quitterButton" layoutX="750.0" layoutY="630.0" onAction="#quitterButtonAction" prefHeight="70.0" prefWidth="140.0" text="Quitter" />
    </children>
</AnchorPane>